#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    
    string s1,s2;
    int flag=1;
    cin>>s1;
    cin>>s2;
    if(s1.length()==s2.length()){
        for(int i=0;i<3;i++)
        {
            if(s1[i]!=s2[i])
                flag=0;
        }
        if(flag==1)
            cout<<"Yes";
        else
            cout<<"No";
    }
    else
        cout<<"No";
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
